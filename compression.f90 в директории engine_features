  end subroutine CompressData

  ! Функция для распаковки сжатых данных
  subroutine DecompressData(compressedData, decompressedData)
    character(len=*), intent(in) :: compressedData
    character(len=*), intent(out) :: decompressedData
    ! Реализация распаковки данных
    ! Твой сложный код с использованием байтов
  end subroutine DecompressData

  ! Дополнительные функции и переменные для сжатия и распаковки данных можно добавить здесь
